package com.superdata.soho.entity;

public class AuditListDetail {
     public String getAuditName() {
		return auditName;
	}
	public void setAuditName(String auditName) {
		this.auditName = auditName;
	}
	public String getAuditResultStr() {
		return auditResultStr;
	}
	public void setAuditResultStr(String auditResultStr) {
		this.auditResultStr = auditResultStr;
	}
	public String getAuditRemark() {
		return auditRemark;
	}
	public void setAuditRemark(String auditRemark) {
		this.auditRemark = auditRemark;
	}
	/**
	 * 查看审批
	 * @param auditName 审批人
	 * @param auditResultStr  审批结果
	 * @param auditRemark     审批意见
	 */
	public AuditListDetail(String auditName, String auditResultStr,
			String auditRemark) {
		super();
		this.auditName = auditName;
		this.auditResultStr = auditResultStr;
		this.auditRemark = auditRemark;
	}
	private String auditName;
     private String auditResultStr;
     private String auditRemark;
     
}
