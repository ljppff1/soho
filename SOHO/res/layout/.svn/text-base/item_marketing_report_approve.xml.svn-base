<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/listviewbg"
    android:paddingBottom="8dp"
    android:paddingLeft="4dp"
    android:paddingRight="4dp"
    android:paddingTop="8dp"
     >
    
    <View
        android:id="@+id/flag"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:tag="1" />
    
    
    <ImageView 
        android:id="@+id/iv_item_marketing_report_approve_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:scaleType="matrix"
        android:src="@drawable/mm_submenu" 
        />

    
   
    
    
    <RelativeLayout 
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_toLeftOf="@id/iv_item_marketing_report_approve_right"
       
        android:layout_marginLeft="2dp"
        >
        
        
        <TextView 
            android:id="@+id/tv_cb_item_marketing_report_approve_names"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="申请人："
            android:textSize="12sp"
            android:textColor="@android:color/black"
            android:paddingLeft="6dp"
            
            />
        
        <TextView 
            android:id="@+id/tv_cb_item_marketing_report_approve_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="王梅亭子"
            android:textSize="12sp"
            android:textColor="@color/blue_1"
            android:layout_toRightOf="@id/tv_cb_item_marketing_report_approve_names"
            android:paddingLeft="2dp"
            />
             <TextView 
            android:id="@+id/tv_cb_item_marketing_report_approve_moneys"
                        android:layout_toRightOf="@id/tv_cb_item_marketing_report_approve_name"
            
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="报告金额："
            android:textSize="12sp"
            android:textColor="@android:color/black"
            android:paddingLeft="20dp"
            
            />
        
        <TextView 
            android:id="@+id/tv_cb_item_marketing_report_approve_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="11234132"
            android:textSize="12sp"
            android:textColor="@color/blue_1"
            android:layout_toRightOf="@id/tv_cb_item_marketing_report_approve_moneys"
            android:paddingLeft="2dp"
            />
        <TextView 
            android:id="@+id/tv_cb_item_marketing_report_approve_themes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="报告标题："
            android:textSize="12sp"
            android:textColor="@android:color/black"
            android:layout_below="@id/tv_cb_item_marketing_report_approve_money"
            android:paddingLeft="6dp"
            android:paddingTop="1dp"
            />
        
        <TextView 
            android:id="@+id/tv_cb_item_marketing_report_approve_theme"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="干 asdfsdf"
            android:textSize="12sp"
             android:maxLines="1"
            android:textColor="@color/blue_1"
            android:layout_toRightOf="@id/tv_cb_item_marketing_report_approve_themes"
            android:layout_below="@id/tv_cb_item_marketing_report_approve_money"
            android:paddingTop="1dp"
            />
        
        <TextView 
            android:id="@+id/tv_cb_item_marketing_report_approve_remarks"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="报告内容："
            android:textSize="12sp"
            android:textColor="@android:color/black"
            android:layout_below="@id/tv_cb_item_marketing_report_approve_theme"
            android:paddingLeft="6dp"
            android:paddingTop="1dp"
            />
        
        <TextView 
            android:id="@+id/tv_cb_item_marketing_report_approve_remark"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无无 "
            android:paddingRight="2dp"
            
            android:textSize="12sp"
            android:maxLines="2"
            android:textColor="@color/blue_1"
            android:layout_toRightOf="@id/tv_cb_item_marketing_report_approve_remarks"
            android:layout_below="@id/tv_cb_item_marketing_report_approve_theme"
			android:paddingTop="1dp"
            />
        
        <TextView 
            android:id="@+id/tv_cb_item_marketing_report_approve_status"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="未通过   +名字"
            android:textSize="13sp"
            android:textColor="@color/red"
            android:layout_below="@id/tv_cb_item_marketing_report_approve_remark"
            android:paddingLeft="6dp"
            android:paddingTop="1dp"
            />
        
        <TextView 
            android:id="@+id/tv_cb_item_marketing_report_approve_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2014/4/10 8:00"
            android:textSize="11sp"
            android:textColor="@color/blue_1"
            android:layout_alignParentRight="true"
            android:layout_below="@id/tv_cb_item_marketing_report_approve_remark"
            android:paddingTop="1dp"
            />
    </RelativeLayout>
    
    

</RelativeLayout>
