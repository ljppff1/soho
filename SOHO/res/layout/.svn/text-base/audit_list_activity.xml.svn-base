    <LinearLayout
        android:id="@+id/audit_list_app_linner_spinner"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_below="@+id/audit_list_app_topitems"
        android:gravity="center_horizontal" >

        <EditText
            android:id="@+id/audit_list_app_etsearch"
            android:layout_height="42dp"
            android:layout_width="0dp"
            android:layout_weight="4"
            android:ems="10"
            android:hint="关键字"
            android:inputType="text"
            android:textSize="12sp" >

            <requestFocus />
        </EditText>

        
        
        <Button
            android:id="@+id/audit_list_app_search"
            android:text="搜索"
             android:layout_marginLeft="5dp"
            android:textSize="12sp" 
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.7"
            android:background="@drawable/public_selector_bg"/>

    </LinearLayout>
        <com.superdata.soho.view.XListView 
            android:layout_below="@id/audit_list_app_linner_spinner"
        android:id="@+id/audit_list_app_listview"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="4dp"
        android:background="@color/transparent"
        ></com.superdata.soho.view.XListView>
    


</RelativeLayout>