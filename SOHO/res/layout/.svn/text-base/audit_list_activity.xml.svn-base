<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
     android:background="@drawable/list_main"
     >
     <LinearLayout
        android:id="@+id/audit_list_app_top"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:background="@drawable/main_list_top"
        android:gravity="center_vertical" >

        <Button
            android:id="@+id/audit_list_lookdetail1_bt_back"
          style="@style/style_title_button_back"/>

        <TextView
            style="@style/TextView.Middle.Black"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="center"
            android:text="审批列表"
            android:textColor="@android:color/white" />
    <Button
             style="@style/style_title_button_back1"
             /> 
    </LinearLayout>
    
    
    
     
    
    
      <LinearLayout
        android:id="@+id/audit_list_app_topitems"
        android:layout_width="fill_parent"
        android:layout_height="39dp"
    android:layout_below="@+id/audit_list_app_top"
        android:layout_margin="10dp" 
        android:visibility="gone"
        android:background="@color/transparent"
        android:gravity="center_vertical" >

                  <com.superdata.soho.view.ComboBoxButton
                  android:id="@+id/audit_list_app_topitems_cb1"
                  android:layout_width="0dp"
                   android:layout_weight="1"
                  android:layout_height="wrap_content"
                  android:background="#ffffff"
                  android:textSize="10dp" />

              <com.superdata.soho.view.ComboBoxButton
                  android:id="@+id/audit_list_app_topitems_cb2"
                  android:layout_width="0dp"
                  android:layout_height="wrap_content"
                   android:layout_weight="1"
                  android:background="#ffffff"
                  android:textSize="10dp" />
          <com.superdata.soho.view.ComboBoxButton
          android:id="@+id/audit_list_app_topitems_cb3"
          android:layout_width="0dp"
          android:layout_height="wrap_content"
          android:layout_weight="1"
          android:background="#ffffff"
          android:textSize="10dp" />
    </LinearLayout>
    
    
