<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
     android:background="@drawable/list_main"
     >
     <LinearLayout
        android:id="@+id/audit_listlookmore_app_top"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:background="@drawable/main_list_top"
        android:gravity="center_vertical" >

        <Button
            android:id="@+id/audit_listlookmore_app_bt_back"
          style="@style/style_title_button_back" />

        <TextView
            style="@style/TextView.Middle.Black"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="center"
            android:text="审批流程"
            android:textColor="@android:color/white" />
    <Button
        style="@style/style_title_button_back1"
             /> 
    </LinearLayout>
    
    
    
        <ListView 
        android:layout_below="@id/audit_listlookmore_app_top"
        android:id="@+id/audit_listlookmore_lv"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
       android:layout_margin="10dp"
           android:background="@color/transparent"
        ></ListView>
    


</RelativeLayout>