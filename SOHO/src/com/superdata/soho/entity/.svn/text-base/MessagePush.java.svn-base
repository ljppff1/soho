package com.superdata.soho.entity;

import java.io.Serializable;

/**
 * 消息推送值对象
 * 
 * @author pengshuo
 * 
 */
public class MessagePush implements Serializable{
	/**
	 * @Fields serialVersionUID 用一句话描述这个变量表示什么
	 */
	private static final long serialVersionUID = 1L;
	
	/**
	 * 消息名称
	 */
	private String msgName;
	/**
	 * 消息ID
	 */
	private String msgId;
	/**
	 * 消息类型
	 */
	private String msgType;
	/**
	 * 公司ID
	 */
	private String companyId;
	/**
	 * 部门ID
	 */
	private String deptId;
	/**
	 * 用户ID
	 */
	private String userId;
	/**
	 * 消息内容
	 */
	private String messageContent;
	/**
	 * 时间
	 */
	private String messageTime;
	
	private String messageTitle;//标题
	

	/**
	 * @return messageTitle
	 */
	public String getMessageTitle() {
		return messageTitle;
	}

	/**
	 * @param messageTitle 要设置的 messageTitle
	 */
	public void setMessageTitle(String messageTitle) {
		this.messageTitle = messageTitle;
	}

	/**
	 * @return messageTime
	 */
	public String getMessageTime() {
		return messageTime;
	}

	/**
	 * @param messageTime 要设置的 messageTime
	 */
	public void setMessageTime(String messageTime) {
		this.messageTime = messageTime;
	}

	public String getMsgName() {
		return msgName;
	}

	public void setMsgName(String msgName) {
		this.msgName = msgName;
	}

	public String getMsgType() {
		return msgType;
	}

	public void setMsgType(String msgType) {
		this.msgType = msgType;
	}

	public String getCompanyId() {
		return companyId;
	}

	public void setCompanyId(String companyId) {
		this.companyId = companyId;
	}

	public String getDeptId() {
		return deptId;
	}

	public void setDeptId(String deptId) {
		this.deptId = deptId;
	}

	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public String getMsgId() {
		return msgId;
	}

	public void setMsgId(String msgId) {
		this.msgId = msgId;
	}

	/**
	 * @return messageContent
	 */
	public String getMessageContent() {
		return messageContent;
	}

	/**
	 * @param messageContent 要设置的 messageContent
	 */
	public void setMessageContent(String messageContent) {
		this.messageContent = messageContent;
	}

}
