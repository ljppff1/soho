package com.superdata.soho.entity;

public class publicnotification {
	private String name;
    private String time;
    private String theme;
    private String remark;

    /**
     * 公告
     * @param name  发布人
     * @param time   时间
     * @param theme   主题 
     * @param remark   内容
     */
	public publicnotification(String name, String time, String theme,
			String remark) {
		super();
		this.name = name;
		this.time = time;
		this.theme = theme;
		this.remark = remark;
	}
     public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getTime() {
		return time;
	}
	public void setTime(String time) {
		this.time = time;
	}
	public String getTheme() {
		return theme;
	}
	public void setTheme(String theme) {
		this.theme = theme;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
     
} 
